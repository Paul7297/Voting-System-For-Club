<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Election Results</title>
  <link rel="stylesheet" href="/css/election-results.css">
</head>
<body>
  <div class="container">
    <header>
      <h1>Election Results</h1> 
    </header>
    <% electionResults.forEach(electionResult => { %>
      <section class="election-result">
        <h2><%= electionResult.electionName %></h2>
        <ul>
          <% for (const candidate in electionResult.candidates) { %>
            <li><%= candidate %>: <%= electionResult.candidates[candidate] %> votes</li>
          <% } %>
        </ul>
      </section>
    <% }); %>
  </div>
</body>
</html>
